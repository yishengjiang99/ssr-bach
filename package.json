{
  "name": "ssr-bach",
  "version": "2.1.0",
  "description": "server-rendered hifi audio",
  "main": "./dist/httpd.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "@tonejs/midi": "^2.0.25",
    "ff32bit": "^1.0.0",
    "flat-cached": "^1.0.1",
    "grep-fifo": "^1.0.5",
    "grep-wss": "^1.1.6",
    "grepupload": "^1.2.0",
    "mime-types": "^2.1.27",
    "mode": "^0.3.2",
    "ssr-cxt": "file:../ssr-ctx"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.2.0",
    "chai": "^4.2.0",
    "commander": "^6.2.1",
    "mocha": "^8.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.2"
  },
  "scripts": {
    "dev": "nodemon -w src/ --exec ts-node src/httpd.ts",
    "test": "mocha",
    "log": "sudo journalctl -u bachd >> server.log",
    "build": "tsc; sudo systemctl enable bachd; sudo systemctl start bachd"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yishengjiang99/ssr-bach.git"
  },
  "mocha": {
    "package": "./package.json",
    "extension": [
      "ts"
    ],
    "spec": [
      "src/*.test.ts"
    ],
    "recursive": true,
    "require": [
      "ts-node/register",
      "./mocha.js"
    ]
  },
  "keywords": [],
