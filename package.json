{
  "name": "ssr-bach",
  "version": "2.1.0",
  "description": "server-rendered hifi audio",
  "main": "./dist/httpd.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "@tonejs/midi": "^2.0.25",
    "ejs": "^3.1.5",
    "ff32bit": "^1.0.0",
    "flat-cached": "^1.0.1",
    "grep-fifo": "^1.0.5",
    "grep-transform": "^1.2.0",
    "grep-wss": "^1.1.6",
    "grepupload": "^1.2.0",
    "mime-types": "^2.1.28",
    "multiparty": "^4.2.2",
    "permessage-deflate": "^0.1.7",
    "redis": "^3.0.2",
    "ssr-cxt": "file:../ssr-ctx",
    "ts-jest": "^26.4.4",
    "websocket-extensions": "^0.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@types/jest": "^26.0.20",
    "babel-jest": "^26.6.3",
    "jest": "^26.6.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "dev": "sudo systemctl stop bachd;  sudo systemctl disable bachd; ps aux |grep httpd |awk '{print $2}'|kill -9; nodemon --exec ts-node src/httpd.ts 443 yisheng",
    "test": "jest --silent=false src/*.ts",
    "log": "sudo journalctl -f -u bachd",
    "build": "npm install ../ssr-ctx; tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yishengjiang99/ssr-bach.git"
  },
  "mocha": {
    "package": "./package.json",
    "extension": [
      "ts"
    ],
    "spec": [
      "src/*.test.ts"
    ],
    "recursive": true,
    "require": [
      "ts-node/register",
      "./mocha.js"
    ]
  },
  "keywords": [],
